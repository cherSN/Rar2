<Window x:Class="MvvmRar.View.RarCompanyView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MvvmRar.View"
        mc:Ignorable="d"
        Title="RarCompanyView" Height="300" Width="300">
    <!--DataContext="{Binding rarViewModel, Source={StaticResource Locator}}" >-->
    <WrapPanel>
        <Label Content="Наименование" />
        <TextBox   Text="{Binding Company.Name}" MinWidth="84" Padding="5" />

    </WrapPanel>
</Window>
